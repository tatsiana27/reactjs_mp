{
  "name": "reactjs_mp",
  "version": "1.0.0",
  "main": "./src/index.js",
  "repository": "https://github.com/tatsiana27/reactjs_mp.git",
  "author": "Tatsiana_Kavaliova@epam.com",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --env.env=dev",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack -p --env.env=prod",
    "test": "jest",
    "test:e2e": "./node_modules/.bin/wdio wdio.conf.js --env local"
  },
  "dependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "file-loader": "1.1.11",
    "jest": "22.4.3",
    "jest-cli": "^22.4.3",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.2.2",
    "semantic-ui-react": "0.79.1",
    "webdriverio": "4.12.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupFiles": [
      "<rootDir>/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "(/_tests_/.*|(\\.|/)(test))\\.jsx?$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/_mocks_/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/_mocks_/styleMock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-core": "6.26.0",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "chalk": "2.4.0",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "html-webpack-plugin": "3.2.0",
    "mocha": "^5.1.1",
    "node-sass": "^4.9.0",
    "postcss-loader": "2.1.4",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "0.11.1",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.1",
    "selenium-server-standalone": "^2.53.1",
    "selenium-standalone": "^6.14.0",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "wdio-jasmine-framework": "^0.3.4",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-spec-reporter": "^0.1.4",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.3",
    "webpack-merge": "4.1.2"
  }
}
